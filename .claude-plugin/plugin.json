{
  "name": "dex",
  "version": "1.0.0",
  "description": "Your AI Chief of Staff. Personal operating system with strategic work management, meeting intelligence, relationship tracking, and daily planning. Built for non-technical professionals who want AI leverage without coding.",
  "author": {
    "name": "Dave Killeen",
    "email": "dave@davekilleen.com",
    "url": "https://github.com/davekilleen"
  },
  "homepage": "https://github.com/davekilleen/dex",
  "repository": "https://github.com/davekilleen/dex",
  "license": "MIT",
  "keywords": [
    "productivity",
    "pkm",
    "knowledge-management",
    "meeting-intelligence",
    "task-management",
    "relationship-tracking",
    "daily-planning",
    "career-development",
    "chief-of-staff",
    "para-method",
    "non-technical",
    "ai-fluency"
  ],
  "mcpServers": {
    "work": {
      "type": "stdio",
      "command": "python",
      "args": ["-m", "core.mcp.work_server"],
      "env": {
        "VAULT_PATH": "${VAULT_PATH}"
      }
    },
    "calendar": {
